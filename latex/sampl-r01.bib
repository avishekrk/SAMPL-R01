  month = jan,
  year = {2014},
  keywords = {alchemical,hydration free energy,SAMPL,SAMPL4},
  pages = {277--287},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/XP55QFZZ/Muddana et al. - 2014 - The SAMPL4 hydration challenge evaluation of part.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/DCVQARXE/s10822-014-9714-6.html:text/html}
}

@article{hamaguchi_force-field_2012,
