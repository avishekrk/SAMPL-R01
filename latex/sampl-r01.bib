  keywords = {Maybridge,RosettaLigand,SAMPL,SAMPL3,trypsin},
  pages = {603--616},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/TNIW4AEA/Kumar and Zhang - 2012 - Computational fragment-based screening using Roset.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/GQ64QI9M/s10822-011-9523-0.html:text/html}
