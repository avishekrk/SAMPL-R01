  pages = {503--510},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/RCIUG324/Ma et al. - 2012 - Acyclic cucurbit[n]uril molecular containers enhan.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/KVG9TN42/nchem.1326.html:text/html}
