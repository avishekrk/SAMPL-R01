  month = feb,
  year = {2014},
  keywords = {alchemical,HIV integrase,SAMPL,SAMPL4},
  pages = {475--490},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/E8RDGITW/Gallicchio et al. - 2014 - Virtual screening of integrase inhibitors by large.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/2W2FXFN7/10.html:text/html}
