  keywords = {hydration free energy,SAMPL,SAMPL4},
  pages = {299--304},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/MKWJQEPR/Fu et al. - 2014 - Fast prediction of hydration free energies for SAM.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/WJZRHG6N/s10822-014-9730-6.html:text/html}
}

