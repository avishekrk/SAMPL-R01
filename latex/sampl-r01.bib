  month = dec,
  year = {2011},
  keywords = {FiSH,hydration free energy,LIE,SAMPL,SAMPL3},
  pages = {661--667},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/JZHMX65Z/Sulea and Purisima - 2011 - Predicting hydration free energies of polychlorina.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/WWZSSNFM/s10822-011-9522-1.html:text/html}
}

