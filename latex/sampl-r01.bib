@article{Jang:2014:Angew.Chem.Int.Ed.,
  title = {Cucurbit[7]uril: {{A High}}-{{Affinity Host}} for {{Encapsulation}} of {{Amino Saccharides}} and {{Supramolecular Stabilization}} of {{Their}} $\alpha$-{{Anomers}} in {{Water}}},
